{
    "dev": {
        "aws_region": "eu-west-2",
        "django_settings": "core.settings",
        "exclude": [
            "boto3",
            "dateutil",
            "botocore",
            "s3transfer",
            "concurrent"
        ],
        "timeout_seconds": 30,
        "project_name": "audiogram-backend",
        "runtime": "python3.10",
        "handler": "wsgi.handler",
        "s3_bucket": "audiogram-django-bucket"
    },
    "production": {
        "aws_region": "eu-west-2",
        "django_settings": "core.settings",
        "slim_handler": false,
        "timeout_seconds": 120,
        "project_name": "audiogram-backend",
        "runtime": "python3.10",
        "handler": "wsgi.handler",
        "s3_bucket": "audiogram-django-bucket"
    }
}